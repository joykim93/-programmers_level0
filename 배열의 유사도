## 배열의 유사도
## 문제 url: https://school.programmers.co.kr/learn/courses/30/lessons/120903

function solution(s1, s2) {
    var answer = 0;
    s1.map((el_s1) => {
        s2.map((el_s2) => {
            if (el_s1 === el_s2) {
                answer++;
            }
        })
    })
    return answer;
}
