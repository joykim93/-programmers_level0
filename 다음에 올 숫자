## 다음에 올 숫자
## 문제 url: https://school.programmers.co.kr/learn/courses/30/lessons/120924

function solution(common) {
    // 등차수열
    if (common[2] - common[1] === common[1] - common[0]) {
        return common[common.length - 1] + (common[1] - common[0]);
    }
    // 등비수열
    else {
        return common[common.length - 1] * (common[1] / common[0]);
    }
}
